<!DOCTYPE html> <html lang="en"> <head> <meta charset="UTF-8"> <meta name="viewport" content="width=device-width, initial-scale=1.0"> <title>Olympiad Dates</title> <style> body { font-family: Arial, sans-serif; text-align: center; margin-top: 50px; } button { margin: 10px; padding: 10px 20px; font-size: 16px; cursor: pointer; border: none; border-radius: 5px; background-color: #4CAF50; color: white; } button:hover { background-color: #45a049; } </style> </head> <body> <h1>Olympiad Subjects</h1> <div> <button onclick="showDate('Math')">Math</button> <button onclick="showDate('English')">English</button> <button onclick="showDate('Science')">Science</button> <button onclick="showDate('Social Science')">Social Science</button> <button onclick="showDate('Cyber')">Cyber</button> <button onclick="showDate('GK')">GK</button> </div> <script> function showDate(subject) { let date; switch (subject) { case 'Math': date = '22 October 2024'; break; case 'English': date = '22 October 2024'; break; case 'Science': date = '3 December 2024'; break; case 'Social Science': date = '23 January 2025'; break; case 'Cyber': date = '21 January 2025'; break; case 'GK': date = '15 October 2024'; break; default: date = 'Unknown'; } alert(subject + ' Date: ' + date); } </script> </body> </html>
